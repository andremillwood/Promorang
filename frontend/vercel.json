{
  "version": 2,
  "buildCommand": "npm install && npm install @rollup/rollup-linux-x64-gnu --force && npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "routes": [
    { "src": "/api/(.*)", "dest": "https://api.promorang.co/api/$1" },
    { "src": "/assets/(.*)", "headers": { "Cache-Control": "public, max-age=31536000, immutable" }, "dest": "/assets/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
